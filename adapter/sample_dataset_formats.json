{
    "sample_image_dataset": {
        "description": "Create folders like this:",
        "structure": {
            "my_dataset/": {
                "cat/": ["cat1.jpg", "cat2.jpg", "cat3.jpg"],
                "dog/": ["dog1.jpg", "dog2.jpg", "dog3.jpg"],
                "bird/": ["bird1.jpg", "bird2.jpg", "bird3.jpg"]
            }
        },
        "notes": "Each sub-folder name = class label. Put images inside. Then zip the folder and upload."
    },
    "sample_text_dataset": {
        "description": "Option A: CSV file",
        "csv_format": "text,label\nThis is a positive review,positive\nThis is negative,negative",
        "description2": "Option B: Folder structure",
        "structure": {
            "my_text_dataset/": {
                "positive/": ["review1.txt", "review2.txt"],
                "negative/": ["review3.txt", "review4.txt"]
            }
        }
    },
    "sample_voice_dataset": {
        "description": "Create folders like this:",
        "structure": {
            "my_audio_dataset/": {
                "hello/": ["hello1.wav", "hello2.wav"],
                "goodbye/": ["goodbye1.wav", "goodbye2.wav"],
                "stop/": ["stop1.wav", "stop2.wav"]
            }
        },
        "notes": "Supported formats: .wav, .mp3, .flac, .ogg, .m4a"
    }
